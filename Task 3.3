import java.util.Arrays;

public class Task4 {

    public static void main(String[] args) {

        int[] array = new int[10];
        array[0] = array[1] = 1;

        for (int i = 2; i < 10; i++) {
            array[i] = array[i - 2] + array[i - 1];
        }
        System.out.println(Arrays.toString(array));
    }
}
